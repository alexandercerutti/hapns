{
	"compilerOptions": {
		"target": "ES2020",
		"module": "Node16",
		"moduleResolution": "Node16",
		"declaration": true,
		"sourceMap": true,
		"outDir": "lib",
		"newLine": "lf",
		"isolatedModules": true,
		"isolatedDeclarations": true,
		"erasableSyntaxOnly": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true
	},
	"include": ["src/**/*"]
}
